#footer {
	width: 100%;
	/*float:left;*/
	background-color: #008e9d;
	text-align: center;
	padding: 40px 0 10px 0;
	font-family: 'segoe_uiregular';
}
#footer ul {
	margin: 0 16px 40px;
	display: inline-block;
	vertical-align: top;
}
#footer ul li {
	border-bottom: 1px solid #80c7ce;
}
#footer ul li a {
	font-size: 16px;
	padding: 8px 40px;
	display: inline-block;
	color: #fff;
	text-decoration: none;
	text-transform: uppercase;
}
#footer ul li a:hover {
	color: #015b64;
}
#footer ul li:last-child {
	border-bottom: none;
}
#footer ul li:first-child {
	text-transform: uppercase;
	font-size: 22px;
	display: inline-block;
	color: #fff;
	text-decoration: none;
	border-bottom: none;
	padding: 0 0 20px;
}
#footer p.social-media {
	display: table;
	margin: 0 auto;
}
#footer p.social-media span {
	display: table-cell;
	vertical-align: middle;
	padding: 0;
}
#footer p.copyright {
	font-size: 16px;
	color: #fff;
	margin: 10px 0;
}
#footer p.social-media a.facebook {
	width: 14px;
	height: 28px;
	display: inline-block;
	background: url(../images/facebook.png) 0 0 no-repeat;
	text-indent: -9999px;
	margin: 5px 5px 8px;
	-webkit-transition: all ease 0.3s;
	-moz-transition: all ease 0.3s;
	-o-transition: all ease 0.3s;
	-ms-transition: all ease 0.3s;
	transition: all ease 0.3s;
}
#footer p.social-media a.facebook:hover {
	background: url(../images/facebook.png) 0 100% no-repeat;
}
#footer p.social-media a.twitter {
	width: 28px;
	height: 24px;
	display: inline-block;
	background: url(../images/twitter.png) 0 0 no-repeat;
	text-indent: -9999px;
	margin: 5px 5px 8px;
	-webkit-transition: all ease 0.3s;
	-moz-transition: all ease 0.3s;
	-o-transition: all ease 0.3s;
	-ms-transition: all ease 0.3s;
	transition: all ease 0.3s;
}
#footer p.social-media a.twitter:hover {
	background: url(../images/twitter.png) 0 100% no-repeat;
}
#footer p.social-media a.pinterest {
	width: 27px;
	height: 33px;
	display: inline-block;
	background: url(../images/pinterest.png) 0 0 no-repeat;
	text-indent: -9999px;
	margin: 5px 5px 8px;
	-webkit-transition: all ease 0.3s;
	-moz-transition: all ease 0.3s;
	-o-transition: all ease 0.3s;
	-ms-transition: all ease 0.3s;
	transition: all ease 0.3s;
}
#footer p.social-media a.pinterest:hover {
	background: url(../images/pinterest.png) 0 100% no-repeat;
}

#header .categories{
	width: 1176px;
	margin: 0 auto 13px;
}
.categories ul{
	list-style:none;
	padding:0;
	margin:0;
}
.categories ul li{
	border:1px solid #e5e5e5;
	text-align:center;
	display:inline-block;
	margin:0 9px 0 0;
	position:relative;
}
.categories ul li a{
	display:block;
	font-size:18px;
	color:#a3a3a3;
	width:145px;
	padding:10px 0;
	font-family: 'segoe_uiregular';
}
.categories ul li:hover a, .categories ul li.active a{
	background:#f1f1f1;
	color:#000;
}

ul.select_drop{
	background:#fff;
	position:absolute;
	left:-1px;
	top:44px;
	width:480px;
	z-index:999;
	padding:15px 20px;
	border:1px solid #e5e5e5;
	display: none;
}
.categories ul li:hover ul.select_drop{
	display:block;
}
ul.select_drop li{
	border:0px solid #e5e5e5;
	text-align:left;
	display:block;
	margin:0px 0 0;
	position:relative;
	width:50%;
	float:left;
	margin-bottom:5px;
}
ul.select_drop li label{
	font-size:15px;
	color:#777777;
	display:inline-block;
}
ul.select_drop li .chck-all{
	display:inline-block;
}
ul.select_drop li .chck-child{
	display:inline-block;
}
ul.select_drop li.blank{
	height:22px;
}

/*Checkbox
------------------------------*/

span.ui-checkbox{
	display: block;
	float: left;
	width:19px;
	height:19px;
	margin:2px 11px 0 0;
	background:url(../images/check-btn.jpg) no-repeat;
	cursor:pointer;
	}
span.ui-radio {
	display: block;
	float: left;
	width:19px;
	height:19px;
	background:url(../images/radio-btn.jpg) no-repeat;
}

span.ui-helper-hidden {
	display: none;
}

span.ui-radio-state-hover,
span.ui-checkbox-state-hover {
	background-position: 0 0px;
}

span.ui-checkbox-state-checked {
	background-position: 0 -19px;
}

span.ui-checkbox-state-checked-hover {
	background-position: 0 -19px;
}
span.ui-radio-state-checked-disabled-hover,
span.ui-radio-state-checked-disabled,
span.ui-radio-state-checked {
	background-position: 0 -19px;
}

span.ui-radio-state-checked-hover {
	background-position: 0 -19px;
}
.ui-radio-disabled,
.ui-checkbox-disabled {
	opacity: 0.7;
}
.ui-helper-hidden-accessible {
	position: absolute;
	left: -999em;
}


