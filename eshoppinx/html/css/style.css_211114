.article-square:nth-of-type(5n+5) {
	margin-right: 0px;
}
.article-square {
	margin: 0 9px 15px;
	padding: 0 8px 0;
	width: 204px;
	float: left;
	overflow: hidden;
	position: relative;
	text-align: right;
	font-family: Tahoma, Arial, Helvetica, sans-serif;
}
.article-square:nth-of-type(5n+1){ margin-left:0px;}
.article-square:nth-of-type(5n+5){ margin-right:0px;}
.article-square.item-rec {
	height: 270px;
}
.article-square > img {
	height: 100%;
	left: 0;
	position: absolute;
	top: 0;
	z-index: 0;
}
.article-square .cover {
	background: none repeat scroll 0 0 #ffffff;
	height: 100%;
	opacity: 0.75;
	position: absolute;
	right: 0;
	top: 0;
	width: 110px;
	z-index: 5;
}
.article-square h3 {
	font-size: 24px;
	color: #f4157e;
	margin:11px 0 0 0;
}
.article-square h3 span {
	color: #5f9111;
	font-size: 36px;
	font-weight: normal;
	font-family: 'rupee_foradianregular';
}
.article-square h4 {
	padding: 5px 0 0 100px;
}
.article-square h4 a {
	color: #000;
	font-size: 17px;
	text-decoration: none;
	font-weight: normal;
}
.article-square h4 a:hover {
	text-decoration: underline;
}
.article-square h4 span {
	display: block;
	padding: 20px 0 0;
}
.article-square h4 span a {
	font-size: 14px;
	color: #000;
	line-height: 26px;
	font-weight: bold;
}
.article-square h4, .article-square h3, .article-square .brand, .article-square .butn-overlay {
	position: relative;
	z-index: 10;
}
.butn-overlay {
	padding:0 6px;
}
.article-square .brand a {
	font-size: 14px;
	text-decoration: none;
	color: #008e9d;
	font-weight: bold;
	padding: 8px 0;
	display: block;
}
.article-square .brand a:hover {
	text-decoration: underline;
}
.article-square .butn-overlay {
	width: 105px;
	height: 45px;
	background-color: #1999a6;
	float: right;
	text-align: center;
	color: #fff;
	font-size: 14px;
	line-height: 20px;
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	padding: 5px 0 0 0;
	margin: 10px 0 0;
	display: none;
}
.article-square .butn-overlay span {
	display: block;
}
.article-square:hover .butn-overlay {
	display: block;
	cursor: pointer;
}
div.row:nth-child(2n+2) .article-square .cover {
	left: 0!important;
}
div.row:nth-child(2n+2) .article-square {
	text-align: left!important;
}
div.row:nth-child(2n+2) .article-square h4 {
	padding: 5px 100px 0 0;
}
div.row:nth-child(2n+2) .butn-overlay {
	float: left;
}
.clear {
	padding: 0;
	margin: 0;
	clear: both;
	overflow:hidden;
}
.nav {
	width: 100%;
	float: left;
	background-color: #f2f2f2;
	min-height: 66px;
}
.nav-wrapper {
	width: 1176px;
	margin: 0 auto;
}
.nav ul {
	margin: 0px;
	padding: 0px;
	text-align: left;
	float: left;
}
.nav ul li {
	display: inline;
	text-align: center;
}
.nav ul li a {
	text-decoration: none;
	line-height: 66px;
	font-size: 17px;
	color: #7f7f7f;
	padding: 0 16px;
	font-family: "Trebuchet MS", Segoe, "Segoe UI", "DejaVu Sans", Verdana, sans-serif;
	display: inline-block;
}
.nav ul li a:hover {
	text-decoration: none;
	background-color: #e9e9e9;
}
.nav ul li a.active {
	background-color: #e9e9e9;
}
.nav-wrapper .search {
	width: 32%;
	float: right;
	margin: 8px 0px;
}
.nav-wrapper .search input[type="text"] {
	width: 80%;
	height: 49px;
	line-height: 49px;
	background-color: #fff;
	border: none;
	float: left;
	padding: 0 8px;
	margin: 0px;
	font-family: 'segoe_uiregular';
	font-size: 18px;
	color: #636363;
	text-transform: uppercase;
}
.nav-wrapper .search input[type="submit"] {
	width: 50px;
	float: left;
	margin: 0px;
	padding: 0px;
	height: 49px;
	background: url(../images/search-butn.jpg) 100% 0 no-repeat;
	border: none;
	cursor: pointer;
	text-indent: -9999px;
}
.nav-2 {
	width: 100%;
	float: left;
	background-color: #777777;
	min-height: 40px;
}
.nav-2-wrapper {
	width: 1176px;
	margin: 0 auto;
}
.nav-2 ul {
	margin: 0px;
	padding: 0px;
	text-align: left;
	float: left;
}
.nav-2 ul li {
	display: inline;
	text-align: center;
}
.nav-2 ul li a {
	text-decoration: none;
	line-height: 40px;
	font-size: 14px;
	color: #ffffff;
	padding: 0 16px;
	font-family: 'segoe_uiregular';
	display: inline-block;
	text-transform: none;
}
.nav-2 ul li a:hover {
	text-decoration: none;
	background-color: #000;
}
.nav-2 ul li a.active {
	background-color: #000;
}
