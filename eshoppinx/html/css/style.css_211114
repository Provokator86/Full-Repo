    position: relative;
    text-transform: uppercase;
}
.owl-next {
    background: none repeat scroll 0 0 #f4157e;
    border-radius: 3px;
    color: #fff;
    float: right;
    font-size: 14px;
    font-weight: bold;
    margin: 20px 15px;
    padding: 5px 20px;
    position: relative;
    text-transform: uppercase;
}




















